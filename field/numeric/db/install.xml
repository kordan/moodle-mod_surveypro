<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/surveypro/field/numeric/db" VERSION="20240227" COMMENT="XMLDB file for Moodle mod/surveypro"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="surveyprofield_numeric" COMMENT="Numeric type surveyprofield">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="defaultvalue" TYPE="float" LENGTH="20" NOTNULL="false" DECIMALS="10" SEQUENCE="false"/>
        <FIELD NAME="signed" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="lowerbound" TYPE="float" LENGTH="20" NOTNULL="false" DECIMALS="10" SEQUENCE="false"/>
        <FIELD NAME="upperbound" TYPE="float" LENGTH="20" NOTNULL="false" DECIMALS="10" SEQUENCE="false"/>
        <FIELD NAME="decimals" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="itemid" TYPE="foreign-unique" FIELDS="itemid" REFTABLE="surveypro_item" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
